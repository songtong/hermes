<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.hermes.portal.dal.role" gen="true">
	<entity name="user-roles" table="user_roles" alias="ur">
		<query-defs>
	     <query name="find-by-username" type="SELECT">
	       <param name="key-username" />
	       <statement><![CDATA[SELECT <FIELDS/>
	       FROM <TABLE/>
	       WHERE <FIELD name='username'/> = ${key-username}]]></statement>
	     </query>
	</query-defs>
	</entity>
</entities>

